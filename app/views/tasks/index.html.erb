
<div class="row">
	<div class="col-6 col-lg-6" id="tasks">
		<h2>Incomplete</h2>
		<div class="tasks" id="incomplete_tasks">
		  <%= render @incomplete_tasks %>
		</div>
		<%= link_to "New Task", new_task_path, class: "btn btn-primary", id: "new_link", remote: true %>
	</div>

	<div class="col-6 col-lg-6">
		<h2>Complete</h2>
		<div class="tasks" id="complete_tasks">
		  <%= render @complete_tasks %>
		</div>
	</div>
</div>
